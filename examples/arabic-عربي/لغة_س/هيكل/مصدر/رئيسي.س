نقطة {
    س: صحيح،
    ص: صحيح 
}

نقطة::^دالة_مرحبا() -> { 
    اطبع_سطر(«مرحبا من دالة!»)
}

نقطة::طريقة_مرحبا() -> {
    اطبع_سطر(«مرحبا من طريقة!»)
}


()-> {
    
    ا:= نقطة { س: ١١ ، ص: ٢ }  
    ب:= نقطة { س: ٣  ، ص: ٥ } 

    اطبع_سطر(ا)
    اطبع_سطر(ب)

    \\ نقطة.دالة_مرحبا()
    ا.طريقة_مرحبا()
}