Point {
    x: int,
    y: int
}

Point::^hello_fn() -> { 
    println("Hello from function!") 
}

Point::hello_method() -> {
    println("Hello from Method!")
} 


()-> {
    p1 := Point { x: 11, y: 2 } 
    p2 := Point { x: 1, y: 5 } 

    println(p1)
    println(p2)
    
    // Point.hello_fn()
    p1.hello_method()

}